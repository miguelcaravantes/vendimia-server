{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": false,
        "member-access": false,
        "ordered-imports": false,
        "quotemark": false,
        "object-literal-sort-keys": false,
        "trailing-comma": false,
        "interface-name": false,
        "max-classes-per-file":false
    },
    "rulesDirectory": []
}